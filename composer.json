{
    "name": "ex-akt/contao-style-bricks-bundle",
    "type": "contao-bundle",
    "description": "Style Bricks Bundle for Contao",
    "license": "LGPL-3.0-or-later",
    "authors": [
        {
            "name": "Samuel Heer",
            "homepage": "https://github.com/samuelheer"
        }
    ],
    "conflict": {
        "contao/core": "*",
        "contao/manager-plugin": "<2.0 || >=3.0"
    },
    "require": {
        "contao/core-bundle": "^4.9 || ^5.0",
        "erdmannfreunde/euf_nutshell": "^1.0",
        "erdmannfreunde/contao-grid-bundle": "^1.0",
        "erdmannfreunde/euf_hero": "^2.0",
        "oveleon/contao-component-style-manager": "^3.2",
        "inspiredminds/contao-glightbox": "^1.0"
    },
    "require-dev": {
        "contao/manager-plugin": "^2.0",
        "friendsofphp/php-cs-fixer": "^2.12"
    },
    "extra": {
        "contao-manager-plugin": "ExAkt\\ContaoStyleBricksBundle\\ContaoManager\\Plugin"
    },
    "suggest": {
        "inspiredminds/contao-wowjs": "^1.0",
        "oveleon/contao-cookiebar": "^1.7"
    },
    "autoload": {
        "psr-4": {
            "ExAkt\\ContaoStyleBricksBundle\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "ExAkt\\ContaoStyleBricksBundle\\Tests\\": "tests/"
        }
    },
    "support": {
        "issues": "https://github.com/ex-akt/contao-style-bricks-bundle/issues",
        "source": "https://github.com/ex-akt/contao-style-bricks-bundle"
    },
    "config": {
        "allow-plugins": {
            "contao-components/installer": true,
            "dealerdirect/phpcodesniffer-composer-installer": true,
            "contao/manager-plugin": true
        }
    }
}
