{
    "name": "ex-akt/contao-bricks-header-bundle",
    "type": "contao-bundle",
    "description": "Bricks Header Bundle for Contao",
    "license": "LGPL-3.0-or-later",
    "authors": [
        {
            "name": "Samuel Heer",
            "homepage": "https://github.com/samuelheer"
        }
    ],
    "require": {
        "php": "7.1",
        "contao/core-bundle": "4.5.*",
        "symfony/framework-bundle": "^3.3"
    },
    "conflict": {
        "contao/core": "*",
        "contao/manager-plugin": "<2.0 || >=3.0"
    },
    "require-dev": {
        "contao/manager-plugin": "^2.0",
        "doctrine/doctrine-cache-bundle": "^1.3",
        "friendsofphp/php-cs-fixer": "^2.12",
        "php-http/guzzle6-adapter": "^1.1",
        "php-http/message-factory": "^1.0.2",
        "phpunit/phpunit": "^5.7.26",
        "symfony/phpunit-bridge": "^3.2"
    },
    "extra": {
        "contao-manager-plugin": "ExAkt\\ContaoBricksHeaderBundle\\ContaoManager\\Plugin"
    },
    "autoload": {
        "psr-4": {
            "ExAkt\\ContaoBricksHeaderBundle\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "ExAkt\\ContaoBricksHeaderBundle\\Tests\\": "tests/"
        }
    },
    "support": {
        "issues": "https://github.com/ex-akt/contao-bricks-header-bundle/issues",
        "source": "https://github.com/ex-akt/contao-bricks-header-bundle"
    }
}
